
(rules PCB mz-80b
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (eu.mihosoft.freerouting.autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 1540)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
  )
  (rule
    (width 200.0)
    (clear 200.2)
    (clear 100.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_500:300_um"
    (shape
      (circle F.Cu 500.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 500.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_500:300_um" "Via[0-1]_500:300_um" default
  )
  (via 
    "Via[0-1]_500:300_um-kicad_default" "Via[0-1]_500:300_um" "kicad_default"
  )
  (via 
    "Via[0-1]_500:300_um-+5V" "Via[0-1]_500:300_um" +5V
  )
  (via 
    "Via[0-1]_500:300_um-GND" "Via[0-1]_500:300_um" GND
  )
  (via_rule
    default "Via[0-1]_500:300_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_500:300_um-kicad_default"
  )
  (via_rule
    +5V "Via[0-1]_500:300_um-+5V"
  )
  (via_rule
    GND "Via[0-1]_500:300_um-GND"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    +5V GND /D2 /D1 /D0 /A15 /A14 /A13
    /A12 /A11 /A10 /A9 /A8 /A7 /A6 /A5
    /A4 /A3 /A2 /A1 /A0 /D3 /D4 /D5
    /D6 /D7 /~M1 /~WR /~RD /~IOREQ /~MREQ /~HALT
    /IE1 /IE0 /RESET /~EXRESET /~EXINT /~EXWAIT /~MNI /phi
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class +5V
    (clearance_class +5V)
    (via_rule +5V)
    (rule
      (width 400.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class GND
    (clearance_class GND)
    (via_rule GND)
    (rule
      (width 400.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)